<% @matches.each do |match| %>
  <%= link_to match.title, match_show_path(match) %><br>
  <%= match.timeanddate.strftime("%m/%d/%Y %H:%M") %><br>
  <%= match.course.name %><br>
  <%= match.users %><br>
<% end %>
<strong><%= link_to "create new match", new_match_path %></strong><br>
<strong><%= link_to "Courses Index", courses_path %><br></strong>
