<% @matches.each do |match| %>
  <h3><%= link_to match.title, match %><br></h3>
  Time and Date:<strong><%= match.date.strftime("%m/%d/%Y")%>   <%= match.time.strftime("%H:%M") %></strong><br>
  <%= match.course.name %><br>
  Users Attending:<br>
  <% match.users.each do |u| %>
    <%= link_to u.email, u %><br>
  <% end %>
<% end %>
<strong><%= link_to "Create New Match", new_match_path %></strong><br>
<strong><%= link_to "Courses Index", courses_path %><br></strong>
